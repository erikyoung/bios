
<div class="booyah-box col-xs-10 col-xs-offset-1">
	<h1 class="heroes text-center">Heroes</h1>
		<%= @bio.name %> 
		<%= @bio.age %> 
		<%= @bio.description %> 
		<%= @bio.hometown %> 
		<%= @bio.hobbies %> 
</div>

<div class="modal-body">
       <%= simple_form_for @photo, :url => bio_photos_path(@bio) do |f| %>
       <%= f.input :picture %>
       <%= f.input :caption %>
      </div>

